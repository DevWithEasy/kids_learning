);

CREATE TABLE Kar (
    id SERIAL PRIMARY KEY,
    order_no INTEGER NOT NULL,
    letter VARCHAR(255) NOT NULL,
    kar VARCHAR(255) NOT NULL,
    image BLOB NOT NULL,
    audio BLOB NOT NULL
);

CREATE TABLE KarLetter (
    id SERIAL PRIMARY KEY,
