);

CREATE TABLE KarWord (
    id SERIAL PRIMARY KEY,
    kar_id INTEGER NOT NULL,
    kar_word VARCHAR(255) NOT NULL,
    FOREIGN KEY (kar_id) REFERENCES Kar(id)
);

